Initial Solution:
class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int count = 1;
        int length = nums.size();
        int i = 1;
        while (i<length){
            if (nums[count]!=nums[count-1])
                count++;
            else
                nums.erase(nums.begin()+count);
            i++;
        }
        return count;
    }
};
