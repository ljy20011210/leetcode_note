Initial Solution:
class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int count = 0;
        int maxnum = 0;
        for (int i=0;i<nums.size();++i){
            if (nums[i]==1){
                count++;
                if (count>maxnum) maxnum = count;
            }
            else{
                count = 0;
            }        
        }
        return maxnum;
    }
};
